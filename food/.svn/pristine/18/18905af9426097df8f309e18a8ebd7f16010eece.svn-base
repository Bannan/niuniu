@charset 'utf-8';
body{font-size:12px;font-family:"Microsoft YaHei";color:#1f1a17;background: #fff;text-decoration:none;color: #333333;}
blockquote,body,button,dd,dl,dt,fieldset,form,img,input,legend,li,ol,pre,td,textarea,th,ul{border:medium none;margin:0;padding:0;outline:0}
button,input,select,textarea{font:14px,"Microsoft YaHei",tahoma,Srial,helvetica,sans-serif;outline:0}
h1,h2,h3,h4,h5,h6{margin:0}
li,ol,ul{list-style:none}
a{text-decoration:none;color: #333333;transition-duration: .3s;
  -webkit-transition-duration: .3s;}
a:hover{text-decoration:none;color: #0095eb;}
p{margin:0}
img{border:0;display:block}
table{border-spacing:0;border-collapse:collapse}
input{border:0}
textarea{resize:none;padding:0}
*, *:before, *:after {
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -o-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
#editor_singcms {width: 100%;}

.login {background: url(../images/login-bg.png) #56bc94;}
.login .login-title {color: #fff;text-align: center;font-size: 20px;margin-top: 200px;}
.login .login-form {max-width: 450px;margin: 10px auto;position: relative;left: -50px;color: #fff;}
.login .login-form .captcha {width: 140px;height: 38px;}
.login .login-form input {background-color: rgba(255, 255, 255, 0.3);border: 1px solid rgba(255, 255, 255, 0.3);color: #fff;}
.login .login-form button {width: 100%;background-color: #048f74;}

.headerWrap .logo {
  position: absolute;
  left: 0px;
  top: 22px;
}
.headerWrap .layui-nav {
  position: absolute;
  right: 0;
  top: 0;
  padding: 0;
  background: none;
}
.site-demo .layui-main {
  margin: 15px;
  line-height: 22px;
}
.footerWrap {
  line-height: 44px;
  font-size: 14px;
}
.slideWrap .layui-icon {
  font-size: 20px;
  margin-top: 2px;
  margin-right: 5px;
  float: left;
}
.layui-layout-admin .layui-side {
  overflow: hidden;
}
.layui-nav-tree .layui-nav-child dd a {
  text-indent: 24px;
}
.layui-elem-quote {
  font-size: 14px;
}
.layui-elem-quote a {
  margin-right: 10px;
}
.pagination {}
.pagination li {display: inline-block;margin-right: -1px;padding: 5px;border: 1px solid #e2e2e2;min-width: 20px;text-align: center;}
.pagination li.active {background: #009688;color: #fff;border: 1px solid #009688;}
.pagination li a {display: block;text-align: center;}

.photo-list {margin: 5px 0;}

.form-container {
  max-width: 400px;
}
.webWrap .layui-form-label {
  width: 100px;
  font-size: 14px;
}
#yan_menu_list .status {
  cursor: pointer;
}
.yan-listorder-btn {
  text-align: center;
  padding: 0px;
  height: 30px;
  font-size: 12px
}

.webuploader-container {
  position: relative;
}
.webuploader-element-invisible {
  position: absolute !important;
  clip: rect(1px 1px 1px 1px); /* IE6, IE7 */
  clip: rect(1px,1px,1px,1px);
}
.webuploader-pick {
  position: relative;
  display: inline-block;
  cursor: pointer;
  background: #009688;
  padding: 8px 15px;
  color: #fff;
  text-align: center;
  border-radius: 3px;
  overflow: hidden;
}
.webuploader-pick-hover {
  background: #009689;
}
.webuploader-pick-disable {
  opacity: 0.6;
  pointer-events:none;
}
